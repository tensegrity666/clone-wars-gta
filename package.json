{
  "name": "gta",
  "version": "1.3.0",
  "description": "RSSchool Final Task",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "npm run prettier && eslint src/**/*.js --fix && stylelint src/**/*.css --syntax css --fix",
    "fastdeploy": "npm run build && netlify deploy --prod",
    "start": "webpack-dev-server",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "prettier": "prettier src/ --write",
    "show-errors": "eslint src/**/*.js && stylelint src/**/*.css --syntax css",
    "release": "standard-version --no-verify",
    "firebase": "npm run lint && npm run build && firebase deploy --only hosting:gta-phaser",
    "serve": "npm run build && firebase serve"
  },
  "keywords": [
    "rsschool",
    "javascript",
    "web-development"
  ],
  "author": "Roman Yakovlev",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@commitlint/cli": "^9.1.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "dependencies": {
    "firebase": "^7.15.5",
    "nanoid": "^3.1.10",
    "netlify": "^4.3.2",
    "phaser": "3.23.0",
    "phaser-animated-tiles": "^2.0.2",
    "phaser3-rex-plugins": "^1.1.19"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint",
      "pre-push": "git pull"
    }
  }
}
